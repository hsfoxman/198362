.post h3.post-title a:visited { color: #ff9900; }

#sidebar a:link    { color: #ff9900;
text-decoration: underline;}
#sidebar a:hover   { color: #000; }
#sidebar a:active  { color: #444; }
#sidebar a:visited { color: #ff9900; }


